<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="10" id="org.rti.papaya.kyrgyzSounds" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Papaya</name>
    <description>
        A simple app that plays sounds and lets you compare your own recorded sounds.
    </description>
    <author email="ict-admin@rti.org" href="http://github.com/ICTatRTI">
        RTI International
    </author>
    <content src="index.html" />
    <icon src="res/icon.png" />
    <access origin="*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <engine name="android" spec="^10.1.2" />
    <plugin name="cordova-plugin-file" spec="^6.0.2" />
    <plugin name="cordova-plugin-media" spec="^5.0.3" />
</widget>
